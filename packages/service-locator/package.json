		"@types/node": "^20.0.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.40.0",
		"jest": "^29.5.0",
		"prettier": "^3.0.0",
		"ts-jest": "^29.1.0",
		"typescript": "^5.0.0"
	}
